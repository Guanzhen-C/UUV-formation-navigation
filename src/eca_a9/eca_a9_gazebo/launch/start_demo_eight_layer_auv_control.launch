  <arg name="radius_3" default="47762"/>
  <arg name="radius_4" default="39427"/>
  <arg name="radius_5" default="38500"/>
  <arg name="radius_6" default="39427"/>
  <arg name="radius_7" default="31181"/>
  <arg name="radius_8" default="30000"/>
