<launch>
  <!-- 多层AUV演示启动文件 - 海面1个+中层8个+海底3个 -->
  
  <!-- =========================== 海面层参数 =========================== -->
  <arg name="surface_x" default="30"/>
  <arg name="surface_y" default="0"/>
  <arg name="surface_z" default="-5"/>
  <arg name="surface_yaw" default="1.5708"/>
  <arg name="surface_radius" default="30"/>
  <arg name="surface_center_x" default="0"/>
  <arg name="surface_center_y" default="0"/>
  <arg name="surface_center_z" default="-5"/>
  <arg name="surface_max_forward_speed" default="2.0"/>
  <arg name="surface_dubins_radius" default="10"/>
  <arg name="surface_n_points" default="100"/>

  <!-- =========================== 海底层参数 =========================== -->
  <arg name="bottom_x1" default="50"/>
  <arg name="bottom_y1" default="0"/>
  <arg name="bottom_z1" default="-50"/>
  <arg name="bottom_yaw1" default="1.5708"/>  <!-- 90 deg -->
  <arg name="bottom_x2" default="-25"/>
  <arg name="bottom_y2" default="43.3"/>
  <arg name="bottom_z2" default="-50"/>
  <arg name="bottom_yaw2" default="3.665"/>   <!-- 210 deg -->
  <arg name="bottom_x3" default="-25"/>
  <arg name="bottom_y3" default="-43.3"/>
  <arg name="bottom_z3" default="-50"/>
  <arg name="bottom_yaw3" default="5.759"/>   <!-- 330 deg -->
  <arg name="bottom_radius" default="50"/>
  <arg name="bottom_center_z" default="-50"/>
  <arg name="bottom_max_forward_speed" default="2.0"/>
  <arg name="bottom_dubins_radius" default="10"/>
  <arg name="bottom_n_points" default="120"/>

  <!-- =========================== 中层参数（原有8个AUV） =========================== -->
  <arg name="x1" default="80"/>
  <arg name="y1" default="0"/>
  <arg name="z1" default="-30"/>
  <arg name="yaw1" default="1.5708"/>
  <arg name="x2" default="56.57"/>
  <arg name="y2" default="56.57"/>
  <arg name="z2" default="-30"/>
  <arg name="yaw2" default="2.356"/>
  <arg name="x3" default="0"/>
  <arg name="y3" default="80"/>
  <arg name="z3" default="-30"/>
  <arg name="yaw3" default="3.1416"/>
  <arg name="x4" default="-56.57"/>
  <arg name="y4" default="56.57"/>
  <arg name="z4" default="-30"/>
  <arg name="yaw4" default="-2.356"/>
  <arg name="x5" default="-80"/>
  <arg name="y5" default="0"/>
  <arg name="z5" default="-30"/>
  <arg name="yaw5" default="-1.5708"/>
  <arg name="x6" default="-56.57"/>
  <arg name="y6" default="-56.57"/>
  <arg name="z6" default="-30"/>
  <arg name="yaw6" default="-0.7854"/>
  <arg name="x7" default="0"/>
  <arg name="y7" default="-80"/>
  <arg name="z7" default="-30"/>
  <arg name="yaw7" default="0"/>
  <arg name="x8" default="56.57"/>
  <arg name="y8" default="-56.57"/>
  <arg name="z8" default="-30"/>
  <arg name="yaw8" default="0.7854"/>
  <arg name="radius" default="80"/>
  <arg name="center_z" default="-30"/>
  <arg name="max_forward_speed" default="2.5"/>
  <arg name="dubins_radius" default="15"/>
  <arg name="n_points" default="150"/>

  <arg name="record" default="false"/>
  <arg name="bag_filename" default="dual_recording.bag"/>
  <arg name="use_ned_frame" default="false"/>

  <!-- =================================================================================== -->
  <!-- 海面层AUV -->
  <!-- =================================================================================== -->
  <group ns="eca_a9_surface">
    <param name="robot_description"
           command="$(find xacro)/xacro '$(find eca_a9_description)/robots/eca_a9_default.urdf.xacro' --inorder
                    namespace:=eca_a9_surface
                    inertial_reference_frame:=world" />

    <node name="urdf_spawner" pkg="uuv_descriptions" type="spawn_model" respawn="false" output="screen"
          args="-urdf -x $(arg surface_x) -y $(arg surface_y) -z $(arg surface_z) -R 0 -P 0 -Y $(arg surface_yaw) -model eca_a9_surface -param /eca_a9_surface/robot_description"/>

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="true" output="screen">
      <param name="robot_description" value="/eca_a9_surface/robot_description" />
    </node>
  </group>

  <include file="$(find uuv_assistants)/launch/message_to_tf.launch">
    <arg name="namespace" value="eca_a9_surface"/>
    <arg name="world_frame" value="world"/>
    <arg name="child_frame_id" value="/eca_a9_surface/base_link"/>
  </include>

  <!-- =================================================================================== -->
  <!-- 中层8个AUV -->
  <!-- =================================================================================== -->
  <!-- SPAWN ECA A9 #1 -->
  <!-- =================================================================================== -->
  <group ns="eca_a9_1">
    <param name="robot_description"
           command="$(find xacro)/xacro '$(find eca_a9_description)/robots/eca_a9_default.urdf.xacro' --inorder
                    namespace:=eca_a9_1
                    inertial_reference_frame:=world" />

    <node name="urdf_spawner" pkg="uuv_descriptions" type="spawn_model" respawn="false" output="screen"
          args="-urdf -x $(arg x1) -y $(arg y1) -z $(arg z1) -R 0 -P 0 -Y $(arg yaw1) -model eca_a9_1 -param /eca_a9_1/robot_description"/>

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="true" output="screen">
      <param name="robot_description" value="/eca_a9_1/robot_description" />
    </node>
  </group>

  <include file="$(find uuv_assistants)/launch/message_to_tf.launch">
    <arg name="namespace" value="eca_a9_1"/>
    <arg name="world_frame" value="world"/>
    <arg name="child_frame_id" value="/eca_a9_1/base_link"/>
  </include>

  <!-- =================================================================================== -->
  <!-- SPAWN ECA A9 #2 (with 3-second delay) -->
  <!-- =================================================================================== -->
  <group ns="eca_a9_2">
    <param name="robot_description"
           command="$(find xacro)/xacro '$(find eca_a9_description)/robots/eca_a9_default.urdf.xacro' --inorder
